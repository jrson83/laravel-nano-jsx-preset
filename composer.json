{
    "name": "jrson83/laravel-nano-jsx-preset",
    "version": "0.1.0",
    "description": "Nano JSX frontend preset for Laravel based on https://github.com/wewowweb/laravel-svelte-preset by Gal Jakic.",
    "keywords": [
        "nano-jsx",
        "laravel",
        "laravel-nano-jsx-preset",
        "jrson83"
    ],
    "homepage": "https://github.com/jrson83/laravel-nano-jsx-preset",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Gal Jakic",
            "email": "gal@wewowweb.com",
            "role": "Original Author"
        },
        {
            "name": "JÃ¶rn Spangenberg",
            "email": "me@jrson.de",
            "role": "Author"
        }
    ],
    "require": {
        "php": "^7.2||^7.3||^7.4||^8.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0",
        "laravel/ui": "^1.2|^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jrson83\\LaravelNanoJsxPreset\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jrson83\\LaravelNanoJsxPreset\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jrson83\\LaravelNanoJsxPreset\\LaravelNanoJsxPresetServiceProvider"
            ]
        }
    }
}
