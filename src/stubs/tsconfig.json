{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react",
    "jsxFactory": "Nano.h",
    "rootDir": "./resources/js",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": false,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["vendor/jrson83/**/**"]
}